{
  "name": "itspirit/payum-payever",
  "type": "library",
  "description": "Payever for Payum",
    "keywords": [
    "payment",
    "payever",
    "payum",
    "ecommerce"
  ],
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Michael Frank",
      "email": "contact@phpdoctor.cc",
      "homepage": "https://www.the-spirit-of-it.com/"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-pdo": "*",
    "ext-pdo_sqlite": "*",
    "ext-sqlite3": "*",
    "payever/sdk-php": "^1.3",
    "payum/core": "^1.5"
  },
  "require-dev": {
    "codeception/codeception": "^2.5",
    "php-http/guzzle6-adapter": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "ItSpirit\\Payum\\Payever\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.5-dev"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
